{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "functions": {
        "app.py": {
          "includeFiles": [
            "app/**"
          ]
        }
      },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app.py"
        }
    ],
    "env": {
        "PYTHONPATH": "app/"
    }
}