{
    "version": 2,
    "cleanUrls": true,
    "trailingSlash": false,
    "builds": [
        {
            "src": "app/app.py",
            "use": "@vercel/python"
        }
    ],
    "rewrites": [
        { "source": "/api/:path*", "destination": "/api/:path*"},
        { "source": "/api", "destination": "/" }
      ]
}