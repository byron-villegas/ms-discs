{
    "buildCommand": "pip install -r requirements.txt",
    "outputDirectory": "dist",
    "devCommand": "flask run",
    "cleanUrls": true,
    "builds": [
        {
            "use": "@vercel/python"
        }
    ],
    "rewrites": [
        {
            "source": "/:path*",
            "destination": "/"
        }
    ]
}